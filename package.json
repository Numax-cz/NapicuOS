{
  "author": "Numax",
  "name": "napicubios",
  "version": "1.6.0",
  "scripts": {
    "start": "ng serve --configuration=production",
    "dev": "ng serve --configuration=development",
    "newapp": "node ./scripts/newapp.js",
    "server-dev": "node ./scripts/server.js dev",
    "server-dev-local": "node ./scripts/server.js dev-local",
    "build": "ng build --build-optimizer --extract-licenses --output-hashing none --optimization true",
    "build-update-api": "node ./scripts/build.js",
    "buildDev": "ng build --configuration=development --build-optimizer --extract-licenses --output-hashing none",
    "build-openapi-dev": "openapi-generator-cli generate -o ./OpenAPI -i http://localhost:8080/v3/api-docs/ -g typescript-angular",
    "build-openapi-prod": "openapi-generator-cli generate -o ./OpenAPI -i https://api-os.napicu.eu/v3/api-docs/ -g typescript-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.5",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "14.2.5",
    "@angular/compiler": "14.2.5",
    "@angular/core": "14.2.5",
    "@angular/forms": "14.2.5",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "14.2.5",
    "@angular/platform-browser-dynamic": "14.2.5",
    "@angular/router": "14.2.5",
    "@angular/service-worker": "14.2.5",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "napicuformatter": "^1.13.2",
    "ng-dynamic-component": "10.5.1",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.5",
    "@angular/cli": "14.2.5",
    "@angular/compiler-cli": "14.2.5",
    "@types/jasmine": "4.3.0",
    "jasmine-core": "~4.4.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "~4.8.4"
  }
}
