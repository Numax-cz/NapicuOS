{
  "author": "Numax",
  "name": "napicubios",
  "version": "1.6.0",
  "scripts": {
    "start": "ng serve --configuration=production",
    "dev": "ng serve --configuration=development",
    "newapp": "node ./scripts/newapp.js",
    "server-dev": "node ./scripts/server.js dev",
    "server-dev-local": "node ./scripts/server.js dev-local",
    "build": "ng build --build-optimizer --extract-licenses --output-hashing none --optimization true",
    "build-update-api": "node ./scripts/build.js",
    "buildDev": "ng build --configuration=development --build-optimizer --extract-licenses --output-hashing none",
    "build-openapi-dev": "openapi-generator-cli generate -o ./OpenAPI -i http://localhost:8080/v3/api-docs/ -g typescript-angular",
    "build-openapi-prod": "openapi-generator-cli generate -o ./OpenAPI -i https://api-os.napicu.eu/v3/api-docs/ -g typescript-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.9",
    "@angular/cdk": "^18.2.10",
    "@angular/common": "18.2.9",
    "@angular/compiler": "18.2.9",
    "@angular/core": "18.2.9",
    "@angular/forms": "18.2.9",
    "@angular/material": "^18.2.10",
    "@angular/platform-browser": "18.2.9",
    "@angular/platform-browser-dynamic": "18.2.9",
    "@angular/router": "18.2.9",
    "@angular/service-worker": "18.2.9",
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "napicuformatter": "^1.14.7",
    "ng-dynamic-component": "10.7.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.10",
    "@angular/cli": "18.2.10",
    "@angular/compiler-cli": "18.2.9",
    "@types/jasmine": "5.1.4",
    "jasmine-core": "~5.4.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "5.5.4"
  }
}
